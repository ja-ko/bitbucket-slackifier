<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:osgi="http://www.springframework.org/schema/osgi"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                           http://www.springframework.org/schema/osgi
                           http://www.springframework.org/schema/osgi/spring-osgi.xsd">

    <osgi:reference id="permissionService" interface="com.atlassian.bitbucket.permission.PermissionService"/>
    <osgi:reference id="navBuilder" interface="com.atlassian.bitbucket.nav.NavBuilder"/>
    <osgi:reference id="soyTemplateRenderer" interface="com.atlassian.soy.renderer.SoyTemplateRenderer"/>
    <osgi:reference id="webResourceUrlProvider" interface="com.atlassian.plugin.webresource.WebResourceUrlProvider"/>
    <osgi:reference id="ao" interface="com.atlassian.activeobjects.external.ActiveObjects"/>
    <osgi:reference id="userService" interface="com.atlassian.bitbucket.user.UserService"/>
    <osgi:reference id="pullRequestService" interface="com.atlassian.bitbucket.pull.PullRequestService"/>
    <osgi:reference id="avatarService" interface="com.atlassian.bitbucket.avatar.AvatarService"/>
</beans>